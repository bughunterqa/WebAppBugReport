@using WebAppBugReport.Data.Models
@model Project

@{
    ViewBag.Title = "Create";
}

<h2 class="row-padding" style="padding-left:450px">Creating project</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()


    <div class="form-horizontal" style="padding-left:410px">
        <hr />

        <div class="form-group">
            <div class="col-md-10">
                <label class="control-label">Name</label>
                @Html.EditorFor(model => model.ProjectName, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.ProjectName, "", new { @class = "text-danger" })
            </div>
        </div>



        <div class="row">
            <div class="col-md-3" >
                <a href="/Projects/Index/" class="btn btn-default">Back to List</a>
            </div>
            <div class="col-md-3" >
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">

            </div>

        </div>
    </div>
}

